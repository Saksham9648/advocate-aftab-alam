{
  "buildCommand": "CI=false vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
    { "source": "/(.*)", "destination": "/" }
  ],
  "trailingSlash": false,
  "cleanUrls": true,
  "github": {
    "silent": true
  }
}
